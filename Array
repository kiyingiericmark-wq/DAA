#include <iostream>
#include <vector>
#include <limits>

int main() {
    int n;
    if (!(std::cin >> n) || n <= 0) {
        std::cerr << "Invalid array size\n";
        return 1;
    }

    std::vector<long long> a(n);
    for (int i = 0; i < n; ++i) {
        if (!(std::cin >> a[i])) {
            std::cerr << "Failed to read array elements\n";
            return 1;
        }
    }

    long long mx = a[0];
    for (int i = 1; i < n; ++i) {
        if (a[i] > mx) mx = a[i];
    }

    std::cout << "Maximum element: " << mx << '\n';
    return 0;
}
